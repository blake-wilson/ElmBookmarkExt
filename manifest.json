{
    "manifest_version": 2,

    "name": "Elm Bookmark Extension",
    "description": "This is a basic tool to manage bookmarks written in Elm",
    "version": "1.0",

    "browser_action": {
    },

    "background": {
      "scripts": ["background.js"]
    },

    "permissions": [
      "*://*.https://ly4uzc77fh.execute-api.us-west-2.amazonaws.com/beta/*",
      "*://*.https://s3-us-west-2.amazonaws.com/yellowpapersun-bookmarks/*",
      "activeTab",
      "bookmarks",
      "tabs"
    ]
  }
